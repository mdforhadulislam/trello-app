{
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "app/app.js": {
      "memory": 3008,
      "maxDuration": 30
    },
    "/*.js": {
      "memory": 3008,
      "maxDuration": 30
    }
  }
}
