{
  "version": 2,
  "builds": [
    {
      "src": "./server.js",
      "use": "@now/node-server"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/.*",
      "dest": "server.js"
    }
  ],

  "functions": {
    "pages/api/hello.js": {
      "memory": 3008,
      "maxDuration": 60
    },
    "pages/api/another.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  }
}
